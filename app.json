{
  "expo": {
    "name": "FS Map Tool",
    "description": "A small tool to know where you're flying in MS Flight Simulator 2020.",
    "privacy": "public",
    "slug": "fsmaptool",
    "version": "1.2.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.riccardolardi.fsmaptool",
      "buildNumber": "1.2.0",
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "package": "com.riccardolardi.fsmaptool",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": "$GOOGLE_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
